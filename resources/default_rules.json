[
  {
    "id": "standard_movie",
    "pattern": "^(?<title>.+?)[. ]\\((?<year>\\d{4})\\).*",
    "type": "movie"
  },
  {
    "id": "standard_tv_s_e",
    "pattern": "^(?<title>.+?)[. ]S(?<season>\\d{1,2})E(?<episode>\\d{1,3}).*",
    "type": "tv"
  },
  {
    "id": "anime_fansub_brackets",
    "pattern": "^\\[.*?\\][ .]* (?<title>.+?)[ .]*\\-?[ .]* (?<episode>\\d{2,4}).*",
    "type": "anime"
  },
  {
    "id": "simple_dash_episode",
    "pattern": "^(?<title>.+?)[ .]*\\-[ .]* (?<episode>\\d{1,3}).*",
    "type": "anime"
  }
]
